{
  "name": "clean-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{,ts,jsx,ts,tsx}": [
      "npm run lintfix",
      "npm run lint",
      "npm run test:staged"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:staged": "jest",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "lintfix": "eslint src/**/*.ts --fix",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "eslint": "^9.4.0",
    "git-commit-msg-linter": "^5.0.8",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
